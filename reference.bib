journal = {IEEE International Conference on Rehabilitation Robotics},
keywords = {computer vision,cylindrical grasp,hand posture,lateral key grip,robotic rehabilitation,tip-to-tip pinch},
pages = {0--3},
pmid = {22275622},
title = {{Computer vision-based classification of hand grip variations in neurorehabilitation}},
