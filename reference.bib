issn = {0897-3806},
journal = {Clinical anatomy (New York, N.Y.)},
keywords = {biomechanics,carpal arch,grip weakness,i d release,s u,z-plrurty},
number = {2},
pages = {100--108},
pmid = {8720784},
title = {{Biomechanical alterations in the carpal arch and hand muscles after carpal tunnel release: a further approach toward understanding the function of the flexor retinaculum and the cause of postoperative grip weakness.}},
