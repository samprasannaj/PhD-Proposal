volume = {69},
year = {1989}
}
@article{Kilgore1989,
abstract = {A functional neuromuscular stimulation system has been developed to provide grasp-release function in quadriplegic individuals. A single command input from the subject controls the stimulus levels to a number of electrodes, thus simultaneously activating several muscles. A method for synthesizing the command input to stimulus output relationship has been developed. The first step involves electrode profiling, which is a method for characterizing the output of an individual electrode/muscle combination. The electrodes are then grouped according to function and a set of rule based procedures is used to synthesize the basic grasp parameters. Results are presented demonstrating the output from lateral and palmar grasps developed by this method. The method has successfully resulted in grasping patterns that can be utilized functionally. Limitations of the method and future improvements are discussed.},
author = {Kilgore, K L and Peckham, P H and Thrope, G B and Keith, M W and Gallaher-Stone, K a},
doi = {10.1109/10.32109},
